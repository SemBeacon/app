{
  "name": "@sembeacon/app",
  "version": "1.0.0",
  "description": "SemBeacon Application",
  "private": false,
  "scripts": {
    "postinstall": "shx rm ./public/js/vendor/openhps/* && shx cp -rf node_modules/@openhps/*/dist/web/*.* ./public/js/vendor/openhps/",
    "serve": "vue-cli-service serve",
    "sync": "ionic cap sync && ionic cap sync @capacitor-community/electron",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "start:android": "ionic capacitor run android",
    "start:ios": "ionic capacitor run ios -l --external",
    "start:electron": "ionic capacitor open @capacitor-community/electron",
    "resources": "capacitor-resources -p android,ios -I"
  },
  "dependencies": {
    "@capacitor-community/bluetooth-le": ">=2.2.3",
    "@capacitor-community/electron": "^4.1.2",
    "@capacitor/android": "4.8.0",
    "@capacitor/app": "4.1.1",
    "@capacitor/core": "4.8.0",
    "@capacitor/device": "^4.1.0",
    "@capacitor/geolocation": "^4.1.0",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/ios": "4.8.0",
    "@capacitor/keyboard": "4.1.1",
    "@capacitor/motion": ">=4.0.2",
    "@capacitor/status-bar": "4.1.1",
    "@ionic/vue": "^7.0.4",
    "@ionic/vue-router": "^7.0.4",
    "@openhps/capacitor-bluetooth": "^0.1.5",
    "@openhps/capacitor-geolocation": "^0.1.5",
    "@openhps/core": "^0.4.20",
    "@openhps/geospatial": "^0.1.17",
    "@openhps/localstorage": "^1.0.1",
    "@openhps/rdf": "^0.3.2",
    "@openhps/rf": "^0.1.29",
    "@openhps/solid": "^0.1.9",
    "@vue-leaflet/vue-leaflet": "^0.9.0",
    "@vueuse/core": "^10.1.0",
    "axios": "^1.3.6",
    "core-js": "^3.30.1",
    "ionicons": "^7.1.0",
    "leaflet": "^1.9.3",
    "pinia": "^2.0.35",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "vue": "^3.2.47",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-property-decorator": "^10.0.0-rc.3",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@capacitor/cli": "4.8.0",
    "@types/jest": "^29.5.1",
    "@types/leaflet": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-e2e-cypress": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/vue3-jest": "^29.2.4",
    "babel-jest": "^29.5.0",
    "capacitor-resources": "^2.0.5",
    "cordova-res": "^0.15.4",
    "cypress": "^12.11.0",
    "eslint": "^8.39.0",
