  "scripts": {
    "postinstall": "npm-run-all --parallel copy:js:* && node ./scripts/process.js",
    "copy:js:openhps": "shx cp -rf node_modules/@openhps/*/dist/web/*.* ./public/js/vendor/openhps/",
    "copy:js:sembeacon": "shx cp -rf node_modules/@sembeacon/*/dist/web/*.* ./public/js/vendor/openhps/",
    "ontologies": "openhps-rdf -ns europont:http://elite.polito.it/ontologies/eupont.owl# -d ./src/ontologies",
    "serve": "vue-cli-service serve",
    "sync": "ionic cap sync && ionic cap sync @capacitor-community/electron",
    "build": "vue-cli-service build",
    "build:android": "ionic capacitor build android --prod",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "start:android": "ionic capacitor run android",
    "start:ios": "ionic capacitor run ios -l --external",
    "start:electron": "ionic capacitor open @capacitor-community/electron",
    "resources": "npx @capacitor/assets generate --splashBackgroundColor #363795 --ios --android --pwa",
    "version": "capacitor-set-version -v 1.4.0 -b 7",
    "update:dependencies": "ncu -u -x vue"
  },
